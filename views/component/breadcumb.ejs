<!-- Breadcrumb -->
<nav class="d-flex mb-4">
    <h6 class="mb-0">
        <% 
        list.unshift("Home")
        list.forEach((item, i) => { 
            if (i >= list.length -1) { 
                let source = item.toLowerCase() %>
                <span class="text-reset"><u><%= item %></u></span>
            <% } else { %>
                <span href="" class="text-reset"><%= item %></span>
                <span>></span>
            <% } 
        }) %>
    </h6>
</nav>
